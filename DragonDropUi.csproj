<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>disable</ImplicitUsings>
    <Company>Farm Credit Services of America</Company>
    <Title>$(AssemblyName)</Title>
    <Copyright>Copyright Â© Farm Credit Services of America 2014</Copyright>
    <Description>A single page angular web app where you can drop pictures into the UI, and then generate a pdf of the images. It is a standalone web app and does not rely on outside services.</Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
